{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "var": {
        "Home": "/home/",
        "Comma": ", ",
        "Tilda": "~",
        "Tformat": "2006,01,02,15,04,05"
    },
    "palette": {
        "py-yellow": "#ffdc4e",
        "py-blue": "#4c8bbe",

        "git-cyan": "#00b0bd",
        "git-lightCyan": "#97D1D4",
        "git-yellow": "#ffc800",
        "git-lightYellow": "#E5CE7D",
        "git-red": "#F54D27",
        "git-lightRed": "#f77558",
        "git-white": "#d8d7ce",

        "ghub-foreground": "#193549",
        "ghub-purple": "#773FC7",
        "ghub-green": "#3FB950",
        "ghub-cyan": "#51dfda",
        "ghub-magenta": "#F778BA",

        "git": "#F3C267",
        "git-modified": "#FF9248",
        "git-diverged": "#FF4500",
        "git-ahead": "#B388FF",
        "git-behind": "#B388FF"
    },
    "blocks": [
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "lightGreen",
                    "foreground_templates": [
                        "{{ if .Root }}white{{ end }}"
                    ],
                    "style": "plain",
                    "template": "<white>{{ if .SSHSession }}ssh {{ end }}</>{{ if .Root }}{{ .UserName }}@{{ else }}<b>{{ .UserName }}@</b>{{ end }}",
                    "type": "session"
                },
                {
                    "type": "command",
                    "style": "plain",
                    "foreground": "lightGreen",
                    "foreground_templates": [
                        "{{ if .Root }}white{{ end }}"
                    ],
                    "template": "{{ if .Root }}{{ .Output }}<lightWhite>:</>{{ else }}<b>{{ .Output }}<lightWhite>:</></b>{{ end }}",
                    "properties": {
                        "shell": "bash",
                        "command": "hostname -I | awk '{print $1}'"
                    }
                },
                {
                    "foreground": "lightBlue",
                    "properties": {
                        "style": "agnoster_full"
                    },
                    "style": "plain",
                    "template": "{{ if contains (nospace (cat (.Var.Home) (.UserName))) (.Location) }}<b>{{ path .Var.Tilda (nospace (cat (.Var.Home) (.UserName))) }}</b>{{ else }}<b><lightRed>/</></b>{{ end }} ",
                    "type": "path"
                },
                {
                    "foreground": "lightWhite",
                    "style": "powerline",
                    "template": "<p:py-blue>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}<p:py-yellow>{{ .Venv }}üêç</>{{ end }}<p:py-blue>{{ .Major }}</>.<p:py-yellow>{{ .Minor }}</>{{ end }}<p:py-blue>)</> ",
                    "type": "python"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [
                {
                    "type": "os",
                    "style": "plain",
                    "foreground": "lightMagenta",
                    "template": "{{ if .WSL }}ü™ü{{ end }} {{.Icon}}",
                    "properties": {
                        "ubuntu": "Ôåú",
                        "mint": "Û∞£≠"
                    }
                },
                {
                    "type": "command",
                    "style": "plain",
                    "foreground": "lightMagenta",
                    "template": "{{ .Output }}",
                    "properties": {
                        "shell": "bash",
                        "command": "arch"
                    }
                },
                {
                    "foreground": "lightWhite",
                    "style": "plain",
                    "type": "shell",
                    "properties": {
                        "mapped_shell_names": {
                            "bash": "üêö",
                            "fish": "üêã",
                            "zsh": "üáø",
                            "pwsh": "<blue>Óûï</blue>"
                        }
                    },
                    "template": "{{ .Name }}"
                },
                {
                    "style": "plain",
                    "foreground": "lightMagenta",
                    "template": "{{.Load1}}{{.HostName}} üêè{{ round ((divf (sub .PhysicalTotalMemory .PhysicalAvailableMemory) 1073741824.0)) .Precision}}/{{ round ((divf .PhysicalTotalMemory 1073741824.0)) .Precision}}üêë{{ round ((divf (sub .SwapTotalMemory .SwapFreeMemory) 1073741824.0)) .Precision}}/{{ round ((divf .SwapTotalMemory 1073741824.0)) .Precision}}",
                    "properties": {
                        "precision": 2
                    },
                    "type": "sysinfo"
                },
                {
                    "foreground": "#ffffff",
                    "foreground_templates": [
                        "{{if eq \"Charged\" .State.String}}#ffffff{{end}}",
                        "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
                        "{{if eq \"Discharging\" .State.String}}#FFFB38{{end}}",
                        "{{if eq \"Not Charging\" .State.String}}#33DD2D{{end}}"
                    ],
                    "properties": {
                        "charged_icon": "üîã",
                        "charging_icon": "üîå",
                        "discharging_icon": "üí®",
                        "not_charging_icon": "üîã"
                    },
                    "style": "plain",
                    "template": "{{ if not .Error }}{{ if ge 10 .Percentage }}ü™´ {{end}}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}%",
                    "type": "battery"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "lightBlue",
                    "foreground_templates": [
                        "{{ if not .Writable }}lightRed{{ end }}",
                        "{{ if .Root }}lightWhite{{ end }}"
                    ],
                    "properties": {
                        "style": "agnoster_full"
                    },
                    "style": "plain",
                    "template": "{{ if .Root }}\"{{ path .Location .Location }}\"{{ else }}<b>\"{{ path .Location .Location }}\"</b>{{ end }} ",
                    "type": "path"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [
                {
                    "foreground": "lightYellow",
                    "properties": {
                        "time_format": "2006,01,02,15,04,05"
                    },
                    "style": "plain",
                    "template": " ({{ .CurrentDate | date .Format }})",
                    "type": "time"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "style": "plain",
                    "type": "git",
                    "template": "<b><p:git-lightRed>Û∞áÄ{{ url .RepoName .UpstreamURL}} </>{{if .LatestTag}}<p:ghub-green>ÔÅÅ{{ .LatestTag }} </>{{end}}<p:ghub-purple>{{ url .UpstreamIcon .UpstreamURL}}{{if not .UpstreamGone }}{{ .Upstream }}{{end}}</> {{ .HEAD }}</b>",
                    "foreground": "p:git-lightYellow",
                    "foreground_templates": [
                        "{{ if (.Staging.Changed) }}p:git-yellow{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-red{{ end }}",
                        "{{ if gt .Ahead 0 }}p:git-lightCyan{{ end }}",
                        "{{ if gt .Behind 0 }}p:ghub-magenta{{ end }}"
                    ],
                    "properties": {
                        "fetch_status": true,
                        "fetch_upstream_icon": true,
                        "fetch_bare_info": true,
                        "github_icon": "Óúà",
                        "branch_icon": "ÔÑ¶"
                    }
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [
                {
                    "style": "plain",
                    "type": "git",
                    "template": "<b>{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} üíª{{ .Working.String }}{{ end }}{{ if .Staging.Changed }} ‚úÖ{{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} üì•{{ .StashCount }}{{ end }}{{if gt .WorktreeCount 0}} {{if not .IsWorkTree}}üè°{{end}}üå≥ÔÄç{{ .WorktreeCount }}{{end}}</b>{{if .IsBare }}üêª{{end}}<p:git-white><b> #{{ substr 0 8 .Commit.Sha }}</b> üñã {{.Commit.Author.Name}}{{if ne .Commit.Committer.Name .Commit.Author.Name }}‚úä{{.Commit.Committer.Name}}{{end}}‚åõ{{ago .Commit.Timestamp}}</>",
                    "foreground": "p:git-lightYellow",
                    "foreground_templates": [
                        "{{ if (.Staging.Changed) }}p:git-yellow{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-red{{ end }}",
                        "{{ if gt .Ahead 0 }}p:git-lightCyan{{ end }}",
                        "{{ if gt .Behind 0 }}p:ghub-magenta{{ end }}"
                    ],
                    "properties": {
                        "fetch_status": true,
                        "fetch_upstream_icon": true,
                        "fetch_bare_info": true,
                        "branch_identical_icon": "Óâπ",
                        "branch_ahead_icon": "ÔÅ¢",
                        "branch_behind_icon": "ÔÅ£",
                        "branch_gone_icon": "Û∞ü¢"
                    }
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "type": "status",
                    "style": "plain",
                    "foreground": "red",
                    "properties": {
                        "status_template": "{{ if gt .Code 0 }}{{ reason .Code }} {{ end }}"
                    },
                    "template": "{{.String}}"
                },
                {
                    "type": "executiontime",
                    // "foreground": "lightWhite",
                    "foreground": "green",
                    "foreground_templates": [
                        "{{ if gt .Code 0 }}red{{ end }}"
                    ],
                    "properties": {
                        "always_enabled": true,
                        "style": "round"
                    },
                    "style": "plain",
                    "template": "{{ .FormattedMs }}<b><f>{{ if gt .Code 0 }}Ó©∂{{else}}ÔêÆ{{ end }}</f></b><white>[</><b>{{.PromptCount}}</b><white>]{{if .Root}}<b>#</b>{{else}}<b>$</b>{{end}}</>"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 2
}
