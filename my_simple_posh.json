{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "var": {
        "Home": "/home/",
        "Tilda": "~",
        "Tformat": "2006,01,02,15,04,05"
    },
    "blocks": [
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "lightGreen",
                    "foreground_templates": [
                        "{{ if .Root }}white{{ end }}"
                    ],
                    "style": "plain",
                    "template": "<white>{{ if .SSHSession }}ssh {{ end }}</>{{ if .Root }}{{ .UserName }}@{{ else }}<b>{{ .UserName }}@</b>{{ end }}",
                    "type": "session"
                },
                {
                    "type": "command",
                    "style": "plain",
                    "foreground": "lightGreen",
                    "foreground_templates": [
                        "{{ if .Root }}white{{ end }}"
                    ],
                    "template": "{{ if .Root }}{{ .Output }}<lightWhite>:</>{{ else }}<b>{{ .Output }}<lightWhite>:</></b>{{ end }}",
                    "properties": {
                        "shell": "bash",
                        "command": "hostname -I | awk '{print $1}'"
                    }
                },
                {
                    "foreground": "lightBlue",
                    "properties": {
                        "style": "agnoster_full"
                    },
                    "style": "plain",
                    "template": "{{ if contains (nospace (cat (.Var.Home) (.UserName))) (.Location) }}<b>{{ path .Var.Tilda (nospace (cat (.Var.Home) (.UserName))) }}</b>{{ else }}<b><lightRed>/</></b>{{ end }} ",
                    "type": "path"
                },
                {
                    "properties": {
                        "mapped_shell_names": {
                            "bash": "üêö",
                            "fish": "üêã",
                            "zsh": "üáø",
                            "pwsh": ">_"
                        }
                    },
                    "template": "{{ .Name }} ",
                    "foreground": "lightWhite",
                    "style": "plain",
                    "type": "shell"
                },
                {
                    "foreground": "lightWhite",
                    "style": "powerline",
                    "template": "üêç<#ffdc4e>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}<#4c8bbe>{{ .Major }}</>.<#ffdc4e>{{ .Minor }}</>{{ end }}<#4c8bbe>)</> ",
                    "type": "python"
                },
                {
                    "foreground": "#F3C267",
                    "foreground_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
                        "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
                        "{{ if gt .Behind 0 }}#B388FF{{ end }}"
                    ],
                    "properties": {
                        "branch_max_length": 25,
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true,
                        "github_icon": "Óúà"
                    },
                    "style": "plain",
                    "template": "{{ if( .UpstreamIcon ) }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} Ó≠ã {{ .StashCount }}{{ end }} ",
                    "type": "git"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [
                {
                    "style": "plain",
                    "foreground": "lightMagenta",
                    "template": "{{.HostName}}:{{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}<b>GB</b>",
                    "type": "sysinfo"
                },
                {
                    "type": "status",
                    "style": "plain",
                    "foreground": "green",
                    "foreground_templates": [
                        "{{ if gt .Code 0 }}lightRed{{ end }}"
                    ],
                    "template": " {{ reason .Code }}"
                },
                {
                    "foreground": "green",
                    "foreground_templates": [
                        "{{ if gt .Code 0 }}lightRed{{ end }}"
                    ],
                    "properties": {
                        "always_enabled": true
                    },
                    "template": " {{ .FormattedMs }}",
                    "style": "plain",
                    "type": "executiontime"
                },
                {
                    "foreground": "green",
                    "foreground_templates": [
                        "{{ if gt .Code 0 }}red{{ end }}"
                    ],
                    "properties": {
                        "always_enabled": true
                    },
                    "style": "plain",
                    "template": "<f>{{ if gt .Code 0 }}Ó©∂{{else}}ÔêÆ{{ end }}</f>",
                    "type": "status"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "lightBlue",
                    "foreground_templates": [
                        "{{ if not .Writable }}lightRed{{ end }}",
                        "{{ if .Root }}lightWhite{{ end }}"
                    ],
                    "properties": {
                        "style": "agnoster_full"
                    },
                    "style": "plain",
                    "template": "{{ if .Root }}\"{{ path .Location .Location }}\"{{ else }}<b>\"{{ path .Location .Location }}\"</b>{{ end }} ",
                    "type": "path"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [
                {
                    "foreground": "lightYellow",
                    "properties": {
                        "time_format": "2006,01,02,15,04,05"
                    },
                    "style": "plain",
                    "template": " ({{ .CurrentDate | date .Format }})",
                    "type": "time"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "style": "plain",
                    "_template": "{{ if( .UpstreamIcon ) }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} Ó≠ã {{ .StashCount }}{{ end }} ",
                    "template_": "\uE0A0 branch_icon \u2261 branch_identical_icon \u2191 branch_ahead_icon \u2193 branch_behind_icon \u2262 branch_gone_icon \uF417 commit_icon \uF412 tag_icon \uE728 rebase_icon \uE29B cherry_pick_icon \uF0E2 revert_icon \uE727 merge_icon \uF594 no_commits_icon \uF408 github_icon \uF296 gitlab_icon \uF171 bitbucket_icon \uEBE8 azure_devops_icon \uF270 codecommit_icon \uF330 codeberg_icon \uE5FB git_icon ",
                    "template": "ÔÑ¶ RepoName {{ .RepoName }} HEAD {{ .HEAD }} Ref {{ .Ref }} BranchStatus {{ .BranchStatus }} {{if not .UpstreamGone }}Upstream {{ .Upstream }} UpstreamIconURL {{ url .UpstreamIcon .UpstreamURL}} {{end}}StashCount {{ .StashCount }} WorktreeCount {{ .WorktreeCount }} IsWorkTree {{ .IsWorkTree }} IsBare {{ .IsBare }}LatestTag {{ .LatestTag }} {{ if .Working.Changed }}WorkingString {{ .Working.String }} {{end}}{{ if .Staging.Changed }}StagingString {{ .Staging.String }} {{end}}--- CommitSubject {{ .Commit.Subject }} CommitSha {{ substr 0 8 .Commit.Sha }} CommitAuthor {{ .Commit.Author.Name }} CommitAuthorEmail {{ .Commit.Author.Email }} CommitCommitter {{ .Commit.Committer.Name }} CommitCommitterEmail {{ .Commit.Committer.Email }} CommitTimestamp {{ .Commit.Timestamp | date .Var.Tformat }}",
                    "reason": [
                        "ÓÇ† branch_icon ‚â° branch_identical_icon ‚Üë branch_ahead_icon ‚Üì branch_behind_icon ‚â¢ branch_gone_icon Ôêó commit_icon Ôêí tag_icon Óú® rebase_icon Óäõ cherry_pick_icon ÔÉ¢ revert_icon Óúß merge_icon Ôñî no_commits_icon Ôêà github_icon Ôäñ gitlab_icon ÔÖ± bitbucket_icon ÓØ® azure_devops_icon Ôâ∞ codecommit_icon Ôå∞ codeberg_icon Óóª git_icon ",
                        "Dropped Ahead {{ .Ahead }} Behind {{ .Behind }} for BranchStatus {{ .BranchStatus }}",
                        "Dropped Detached {{ .Detached }} Merge {{ .Merge }} Rebase {{ .Rebase }} CherryPick {{ .CherryPick }} Revert {{ .Revert }} for HEAD {{ .HEAD }}",
                        "Dropped Dir {{ path .UpstreamIcon .Dir }} Kraken {{ url .UpstreamIcon .Kraken }} "
                    ],
                    "properties": {
                        "fetch_status": true,
                        "fetch_upstream_icon": true,
                        "fetch_bare_info": true,
                        "github_icon": "Ôêà",
                        "fetch_user": true
                    },
                    "type": "git"
                },
                {
                    "foreground": "lightWhite",
                    "style": "plain",
                    "template": "[{{.PromptCount}}]{{if .Root}}<b>#</b>{{else}}<b>$</b>{{end}}",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 2
}
